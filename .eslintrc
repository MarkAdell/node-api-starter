{
  "extends": "airbnb-base",
  "rules": {
    "global-require": 0,
    "arrow-body-style": 0,
    "import/order": 0,
    "consistent-return": 0,
    "func-names": 0,
    "prefer-destructuring": 0,
    "newline-per-chained-call": 0,
    "no-restricted-syntax": 0,
    "no-await-in-loop": 0,
    "linebreak-style": 0,
    "no-param-reassign": 0,
    "max-len": 0,
    "no-console": 0
  },
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 8
  }
}
